<ion-header no-border>
  <ion-toolbar>
    <ion-buttons slot="start">
      <ion-back-button>kdlkaJS</ion-back-button>
    </ion-buttons>
    <ion-title>Reservas Tur√≠sticas</ion-title>
  </ion-toolbar>
</ion-header>
<ion-content padding>
  <ion-toolbar>
    <ion-buttons slot="secondary">
      <ion-button (click)="newReserva()"  routerDirection="forward">
        <ion-icon color="primary" size="large" name="add-circle-sharp"></ion-icon>
      </ion-button>
      <ion-button (click)="listarReservas()"  routerDirection="forward">
        <ion-icon color="primary" name="reader-outline"  size="large"></ion-icon>
      </ion-button>
      <ion-button color="secondary">
        <ion-icon slot="icon-only" ios="ellipsis-horizontal" md="ellipsis-vertical"></ion-icon>
      </ion-button>
    </ion-buttons>    
  </ion-toolbar>
 
 
  <router-outlet>
    
  </router-outlet> 
</ion-content>

